name = 'Blat'
version = '35'

homepage = 'http://users.soe.ucsc.edu/~kent/src/'
description = """Blat can be used for merging (pairwise alignment of contigs to remove redundant contigs) and aligning
contigs to a reference genome."""

toolkit = {'name':'goalf','version':'1.1.0-no-OFED'}

sources = ['%sSrc%s.zip' % (name.lower(), version)]
sourceURLs = [homepage]

patches = ['blat_home_fix.patch']


dependencies = [
                ('libpng','1.5.10'),
                ]
